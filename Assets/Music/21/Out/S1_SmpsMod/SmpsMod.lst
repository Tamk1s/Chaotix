                        smpsHeaderPWM macro loc
                        	dc.w	loc-songStart
                        	endm
                        	
                        ; Header - Set up first PWM Channel
                        smpsHeaderPWM1 macro loc,num
                        	dc.w	loc-songStart
                        	dc.b	num,0
                        	endm
                        	
